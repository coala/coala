Default.enabled = false
PythonCheck.enabled = true
# LineCounting is disabled because of inherited value from Default

[Default]
files = **/*.py

[PythonCheck]
bears = KeywordBear, SpaceConsistencyBear, LineLengthBear
cs_keywords = 
ci_keywords = \#TODO, \# TODO, \#FIXME, \# FIXME
use_spaces = True
max_line_length = 120

[LineCounting]
bears = LineCountBear
